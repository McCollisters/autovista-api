{
  "name": "autovista-api",
  "version": "1.0.0",
  "description": "McCollister's Autovista API",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "NODE_ENV=development tsx --watch src/index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "build": "tsc || true",
    "postbuild": "node fix-imports.cjs",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:ci": "NODE_ENV=test jest --ci --coverage --watchAll=false",
    "migration:run": "tsx migrations/run-migration.ts",
    "migration:list": "tsx migrations/run-migration.ts list",
    "migration:up": "tsx migrations/run-migration.ts",
    "migration:down": "tsx migrations/run-migration.ts up down",
    "migrate:all": "tsx -r dotenv/config migrations/scripts/migrate-all.ts",
    "migrate:quotes": "tsx -r dotenv/config migrations/scripts/migrate-quotes.ts",
    "migrate:quotes:down": "tsx -r dotenv/config migrations/scripts/migrate-quotes.ts down",
    "migrate:orders": "tsx -r dotenv/config migrations/scripts/migrate-orders.ts",
    "migrate:orders:down": "tsx -r dotenv/config migrations/scripts/migrate-orders.ts down",
    "migrate:orders:last5": "tsx -r dotenv/config migrations/scripts/migrate-orders-last5days.ts",
    "migrate:orders:last5:down": "tsx -r dotenv/config migrations/scripts/migrate-orders-last5days.ts down",
    "migrate:notification-logs": "tsx -r dotenv/config migrations/scripts/migrate-notification-logs.ts",
    "migrate:notification-logs:down": "tsx -r dotenv/config migrations/scripts/migrate-notification-logs.ts down",
    "migrate:portals": "tsx -r dotenv/config migrations/scripts/migrate-portals.ts",
    "migrate:portals:down": "tsx -r dotenv/config migrations/scripts/migrate-portals.ts down",
    "migrate:modifier-sets": "tsx -r dotenv/config migrations/scripts/migrate-modifier-sets.ts",
    "migrate:modifier-sets:down": "tsx -r dotenv/config migrations/scripts/migrate-modifier-sets.ts down",
    "migrate:users": "tsx -r dotenv/config migrations/scripts/migrate-users.ts",
    "migrate:users:down": "tsx -r dotenv/config migrations/scripts/migrate-users.ts down",
    "migrate:surveys": "tsx -r dotenv/config migrations/scripts/migrate-surveys.ts",
    "migrate:surveys:down": "tsx -r dotenv/config migrations/scripts/migrate-surveys.ts down",
    "migrate:survey-responses": "tsx -r dotenv/config migrations/scripts/migrate-survey-responses.ts",
    "migrate:survey-responses:down": "tsx -r dotenv/config migrations/scripts/migrate-survey-responses.ts down"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/client-sns": "^3.936.0",
    "@aws-sdk/client-sqs": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "google-auth-library": "^10.5.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "mongoose": "^8.19.0",
    "mongoose-sequence": "^6.0.1",
    "nanoid": "^5.1.6",
    "node-cron": "^4.2.1",
    "objects-to-csv": "^1.3.6",
    "tsx": "^4.20.6",
    "twilio": "^5.10.6",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@jest/globals": "^30.2.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/mongoose-sequence": "^3.0.11",
    "@types/node": "^24.6.2",
    "@types/node-cron": "^3.0.11",
    "@types/objects-to-csv": "^1.3.3",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "esbuild": "^0.25.10",
    "i": "^0.3.7",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.2.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
